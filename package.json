{
  "name": "boomslanger",
  "version": "0.1.1",
  "author": "=",
  "dependencies": {
    "humps": "^2.0.1",
    "lodash.isplainobject": "^4.0.6",
    "pg-promise": "^10.8.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "@types/humps": "^2.0.0",
    "@types/lodash.isplainobject": "^4.0.6",
    "husky": "^4.3.0",
    "np": "^7.0.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/boomslanger.esm.js",
  "peerDependencies": {},
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "pretest": "docker-compose up -d",
    "release": "np",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "size-limit": [
    {
      "path": "dist/boomslanger.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/boomslanger.esm.js",
      "limit": "10 KB"
    }
  ],
  "typings": "dist/index.d.ts"
}
